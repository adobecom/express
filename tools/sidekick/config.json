{
  "project": "Express",
  "host": "www.adobe.com",
  "byocdn": true,
  "plugins": [
    {
      "id": "metadata",
      "title": "Meta Data Inspector",
      "url": "https://main--express--adobecom.hlx.page/tools/metadata/inspector.html",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**metadata.xlsx**" ]
    },
    {
      "id": "templates",
      "title": "Templates",
      "url": "https://main--express--adobecom.hlx.page/tools/templates/picker.html",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**.docx**" ]
    },
    {
      "id": "library",
      "title": "Library",
      "environments": [ "edit" ],
      "isPalette": true,
      "paletteRect": "top: auto; bottom: 25px; left: 25px; height: 388px; width: 360px;",
      "url": "https://main--express--adobecom.hlx.live/docs/library/",
      "excludePaths": [ "/**" ],
      "includePaths": [ "**.docx**" ]
    },
    {
      "id": "docs",
      "title": "Docs",
      "url": "https://main--express--adobecom.hlx.live/docs/",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**.docx**" ]
    },
    {
      "id": "tools",
      "title": "Tools",
      "isContainer": true
    },
    {
      "containerId": "tools",
      "id": "qa-guide",
      "title": "QA Guide",
      "event": "qa-guide",
      "excludePaths": [ "**.docx**", "**/:x:**" ]
    },
    {
      "containerId": "tools",
      "title": "Preflight",
      "id": "preflight",
      "environments": ["dev", "preview", "live"],
      "event": "preflight"
    },
    {
      "id": "localeflow",
      "title": "Translate",
      "environments": [
        "dev"
      ],
      "event": "locale-flow",
      "includePaths": [
        "**.docx**"
      ]
    },
    {
      "containerId": "tools",
      "id": "blocknamehighlight",
      "title": "Show Block Names",
      "environments": [
        "dev",
        "preview",
        "live"
      ],
      "event": "block-name-highlight"
    }
  ]
}
